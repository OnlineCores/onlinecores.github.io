extends layout

block content

  //- HEAD SECTION
  div(id='index-banner', class='section no-pad-bot')
    div(class='container')
      h1(class='center-on-small-only white-text')
        b= title
      div(class='row')
        h4(class='col s12 white-text') MIT Licens
      div(class='row center hide')
        a(id='download-button', class='btn-large waves-effect waves-light', href='#!') DOWNLOAD
      div(class='row center hide')
        a(class='blue-text text-lighten-4', href='#!') alpha release v0.00.0
    div(class='github-commit hide')
      div(class='container')
        div(class='commit')
          | Latest Commit on Github: &nbsp;&nbsp;
          a(class='sha', href='#!') &nbsp;&nbsp;
          span(class='date')
          a(id='github-button', class='btn-flat right grey-text text-lighten-5 waves-effect waves-light hide-on-small-only', href='#!') Github

  div(class='row container')
    div(class='col s12 m4') dfds